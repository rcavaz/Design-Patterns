@startuml
hide empty attributes
hide empty members
skinparam backgroundcolor White
skinparam class {
 ArrowColor Black
 BackgroundColor White
 BorderColor Black
}

class Singleton {
 -singletonData
 -uniqueInstance
 +static intance()
 +getSingletonData()
}
note right
<b>instance()</b> {
  return uniqueInstance
}
end note
@enduml
